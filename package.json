{
  "name": "themeable-contact-form",
  "description": "A simple contact form plugin for WordPress that allows you to customize the template to match your theme",
  "version": "0.2.0",
  "author": "Rex Rana Design and Development",
  "contributors": [
    {
      "name": "Peter Hebert",
      "email": "peter@rexrana.ca"
    }
  ],
  "keywords": [
    "wordpress",
    "plugin",
    "contact form",
    "contact",
    "form",
    "bootstrap",
    "foundation"
  ],
  "license": "GPL-2.0+",
  "main": "Gruntfile.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rexrana/themeable-contact-form.git"
  },
  "devDependencies": {
    "grunt": "^1.5.3",
    "grunt-wp-i18n": "^1.0.3",
    "grunt-wp-readme-to-markdown": "^2.1.0"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "test:php": "wp-env run tests-wordpress phpunit 'phpunit -c /var/www/html/wp-content/plugins/$(basename \"$PWD\")/phpunit.xml.dist --verbose'",
    "wp-env": "wp-env",
    "i18n": "grunt i18n",
    "readme": "grunt readme"
  }
}
