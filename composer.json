{
    "name": "rexrana/themeable-contact-form",
    "description": "A simple contact form plugin for WordPress that allows you to customize the template to match your theme",
    "keywords": ["wordpress", "plugin", "contact form", "contact", "form", "bootstrap", "foundation"],
    "type": "wordpress-plugin",
    "license": "GPL-2.0+",
    "authors": [
      {
        "name": "Rex Rana Design and Development Ltd.",
        "homepage": "https://rexrana.ca"
      },
      {
        "name": "Peter Hebert",
        "email": "peter@rexrana.ca",
        "homepage": "https://peterhebert.com/",
        "role": "Developer"
      }
    ],
    "require": {
      "php": ">=7.0",
      "composer/installers": "*",
      "gamajo/template-loader": "^1.3",
      "wixel/gump": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "wp-coding-standards/wpcs": "^2.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2"
    },
    "autoload": {
      "psr-4": {
        "RexRana\\ThemeableContactForm\\": ["src/ThemeableContactForm"],
        "dependencies\\": ["src/dependencies/"]
      }
    },
    "autoload-dev": {
        "psr-4": {
          "RexRana\\ThemeableContactForm\\Tests\\": ["tests/"]
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
